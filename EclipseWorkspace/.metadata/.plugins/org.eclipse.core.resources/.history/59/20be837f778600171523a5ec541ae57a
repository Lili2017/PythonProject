#import pandas as pd
import numpy as np
from test.test_audioop import minvalues
from pandas.compat.numpy import np_array_datetime64_compat


def CalculateData(data):
    aver=np.mean(data)
    stdva=np.std(data,ddof = 1)
    maxval=np.max(data)
    minval=np.min(data)
    return aver,stdva,maxval,minval

testdata=[1,2,3,4,5]
avervalue,stdvalue,maxval,minval=CalculateData(testdata)
print avervalue
print stdvalue
print maxval
print minval
